{
  "name": "code-with-me",
  "version": "1.0.0",
  "description": "pair coding matcher for silicon-slopes-engineer-workshop meet up",
  "main": "index.js",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node index.js",
    "start:prod": "node index.js",
    "seed": "cd api && cd seeds && node taskSeeds.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd .. && cd client && npm run start",
    "test": "nodemon --exec 'mocha --recursive -R min'",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "author": "silicon-slopes-engineer-workshop",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/silicon-slopes-engineer-workshop/coding_pairs.git"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cluster": "^0.7.7",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "mocha": "^6.1.4",
    "mongoose": "^5.2.15",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {},
  "keywords": [
    "pair",
    "node",
    "heroku",
    "create-react-app",
    "react",
    "mern",
    "mongodb"
  ]
}
